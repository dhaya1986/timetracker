/*Form Classes*/
.form-control
{
	font-weight:400!important;
	display:initial;
	width:100%;
	height:34px;
	padding:6px 12px;
	font-size:12px;
	line-height:1.42857143;
	color:#555;
	background-color:#fff;
	background-image:none;
	border:1px solid #ccc;
	border-radius:4px;
	box-shadow:inset 0 1px 1px rgba(0,0,0,.075);
	box-shadow ease-in-out .15s;
	transition:border-color ease-in-out .15s,
}

.jj-input-text{padding:5px;min-width:180px;border-radius:6px;border:1px solid #ccc;display:block;margin-right:-5px;box-sizing:border-box}

/*dropdown css*/

.jj-dropdown{border:1px solid #ccc}
.jj-dropdown-links{width:120px;border-radius:8px!important;border:1px solid #ccc!important;background-color:#fff;padding:8px!important;text-align:left!important;z-index:5;text-decoration:none;position:absolute;right:0}
.jj-dropdown-links li,.jj-dropdown-links a{background:none!important;display:block!important;min-width:100px!important;text-decoration:none!important;color:#808184!important;padding:1px!important}

input[type='checkbox']{width:16px!important;height:14px!important;margin-left:2px;cursor:pointer}
textarea:focus{border-color:#01628F!important;outline:0!important;box-shadow:inset 0 0 0 rgba(0,0,0,0),0 0 0 rgba(0,175,233,0)!important}
input[type='text']:focus,select:focus{outline:none;border-color:#01628F}
    
.jj-textarea{min-height:50px;resize:none;border:1px solid #ccc;border-radius:6px}
.jj-textarea:focus{border:1px solid #01628F;outline:none}


