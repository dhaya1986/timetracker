/*Tiles Classes*/
	
	/*Generic tile according to ink*/
		
	.jj-tile, .jj-tile-blue{
			min-height:150px;
			border-radius:5px;
			width:225px;
			padding:10px 10px 5px 10px;
			box-sizing:border-box;
			float:left;
			box-shadow:1px 1px 5px #B3B3B3;
			display:block;
			color:#808184;
			margin:0px 15px 15px 0px;
			position:relative;
			line-height: 1.5em;
		}
		.jj-tile a.favorite, .jj-tile-blue a.favorite{
			right:0px !important;
			top:-3px !important;
			position:relative;
		}
		.jj-tile:hover, .jj-tile-blue:hover{
			box-shadow:1px 1px 5px #2D86C2;
		}
		.jj-tile img, .jj-tile-blue img{
			width:100%;
		}
		.jj-tile h3 a, .jj-tile-blue h3 a{
			width:23px;
			float:right;
		}
		.jj-tile-blue, .jj-tile-blue a{
			background-color:#01628F;
			color:#fff;
		}
		.jj-tile-progress-bar{
			display:flex;
			margin:0px 1px;
			height:8px;
		}
		.jj-tile-progress-step{
			flex:1;
			background-color:#ccc;
			margin-right:2px;
		}
		.jj-tile-progress-step.jj-completed{
			background-color:#019ed1;
		}
		.jj-tile a.favorite, .jj-tile-blue a.favorite{
			background:url(/ui/skins/CG510_Common_Skin/images/Favorite.png) top left no-repeat;
			display:inline-block;
			width:25px;
			height:25px;
			position:relative;
			top:3px;
			overflow:hidden;
			text-indent:-100em;
		}
		.jj-tile a.favorite:hover, .jj-tile-blue a.favorite:hover{
			background:url(/ui/skins/CG510_Common_Skin/images/Favorite_On.png) top left no-repeat;
		}
		
		
